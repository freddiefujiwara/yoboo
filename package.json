{
  "name": "yoboo",
  "version": "0.0.9",
  "description": "Yaml based task runner",
  "scripts": {
    "clean": "rimraf lib",
    "test": "cross-env BABEL_ENV=commonjs mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "lint": "eslint -c google src test",
    "build": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "coverage": "mkdir -p .nyc_output && nyc report --reporter=text-lcov > coverage.lcov && codecov -t 8423f48e-506b-4d39-b449-fa7323b1bbb5",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run coverage && npm run build"
  },
  "bin": {
    "yoboo": "index.js"
  },
  "files": [
    "lib",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freddiefujiwara/yoboo.git"
  },
  "keywords": [
    "headless chrome",
    "task runner",
    "es2015"
  ],
  "author": "Fumikazu Fujiwara <npm@ze.gs> (http://freddiefujiwara.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/freddiefujiwara/yoboo/issues"
  },
  "homepage": "https://github.com/freddiefujiwara/yoboo#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.0.2",
    "codecov": "^2.2.0",
    "cross-env": "^5.0.1",
    "eslint": "^4.1.1",
    "eslint-config-google": "^0.9.1",
    "mocha": "^3.4.2",
    "nyc": "^11.1.0",
    "rimraf": "^2.6.1"
  },
  "main": "lib/yoboo.js",
  "dependencies": {
    "js-yaml": "^3.9.0"
  }
}

